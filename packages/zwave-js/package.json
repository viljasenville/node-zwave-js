{
  "name": "zwave-js",
  "version": "7.10.1",
  "description": "Z-Wave driver written entirely in JavaScript/TypeScript",
  "keywords": [],
  "main": "build/cjs/index.js",
  "module": "build/esm/index.js",
  "exports": {
    ".": {
        "import": "./build/esm/index.js",
        "require": "./build/cjs/index.js"
    }
  },
  "types": "build/esm/index.d.ts",
  "files": [
    "build/**/*.{js,d.ts,map}",
    "index.{js,d.ts,map}",
    "{CommandClass,Controller,Driver,Error,Node,Utils,Values}.{js,d.ts,map}"
  ],
  "author": {
    "name": "AlCalzone",
    "email": "d.griesel@gmx.net"
  },
  "license": "MIT",
  "homepage": "https://github.com/AlCalzone/node-zwave-js#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AlCalzone/node-zwave-js.git"
  },
  "bugs": {
    "url": "https://github.com/AlCalzone/node-zwave-js/issues"
  },
  "funding": {
    "url": "https://github.com/sponsors/AlCalzone/"
  },
  "engines": {
    "node": ">=v12.22.2"
  },
  "scripts": {
    "b": "yarn ts maintenance/_build.ts",
    "build_full": "yarn b prebuild:full && yarn build && yarn b postbuild:full",
    "build": "tsc -b tsconfig.build.json",
    "postbuild": "node ../../maintenance/esm2cjs.js --in=build/esm --out=build/cjs",
    "clean": "tsc -b tsconfig.build.json --clean",
    "prewatch": "yarn b prebuild",
    "watch": "tsc -b tsconfig.build.json --watch --pretty",
    "lint_zwave": "yarn b lint",
    "ts": "node -r esbuild-register"
  },
  "dependencies": {
    "@alcalzone/jsonl-db": "^1.3.0",
    "@alcalzone/pak": "^0.6.0",
    "@sentry/integrations": "^6.8.0",
    "@sentry/node": "^6.8.0",
    "@zwave-js/config": "7.10.1",
    "@zwave-js/core": "7.10.1",
    "@zwave-js/serial": "7.10.1",
    "@zwave-js/shared": "7.10.1",
    "alcalzone-shared": "^4.0.0",
    "ansi-colors": "^4.1.1",
    "axios": "^0.21.1",
    "execa": "^5.1.1",
    "fs-extra": "^10.0.0",
    "proper-lockfile": "^4.1.2",
    "reflect-metadata": "^0.1.13",
    "semver": "^7.3.5",
    "serialport": "^9.2.0",
    "source-map-support": "^0.5.19",
    "winston": "^3.3.3",
    "xstate": "^4.20.2"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.11",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.12.5",
    "@types/proper-lockfile": "^4",
    "@types/semver": "^7.3.6",
    "@types/serialport": "^8.0.1",
    "@types/source-map-support": "^0",
    "@xstate/test": "^0.4.2",
    "@zwave-js/maintenance": "7.10.1",
    "@zwave-js/testing": "7.10.1",
    "esbuild-register": "^2.6.0",
    "jest-extended": "^0.11.5",
    "mockdate": "^3.0.5",
    "typescript": "^4.3.5"
  }
}
